    "HostGroup": {
        "hstg_type_device": {
            "assign_filter": "\"Device\"=host.templates",
            "display_name": "Type: Devices",
            "object_name": "hstg_type_device",
            "object_type": "object"
        },
        "hstg_type_virtual_machine": {
            "assign_filter": "\"Virtual Machine\"=host.templates",
